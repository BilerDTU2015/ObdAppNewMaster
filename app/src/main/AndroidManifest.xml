<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.hamed.obdappnewmaster" >

    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission
        android:name="com.example.androidmapsv2.permission.MAPS_RECEIVE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/obdblue"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".BluetoothActivity"
            android:label="Bluetooth terminal"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity" >
        </activity>

        <activity
            android:name=".CarPower"
            android:label="fuel_electricity"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity" >
        </activity>

        <activity
            android:name=".SpeedActivity"
            android:label="speed_velocity"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity" >
        </activity>

         <activity
            android:name=".SupportActivity"
            android:label="supporting"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity" >
         </activity>

        <activity
            android:name=".TempActivity"
            android:label="temperature"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity" >
        </activity>

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.hamed.obdappnewmaster.DataActivity"
            android:label="Data"
            android:parentActivityName="com.example.hamed.obdappnewmaster.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <service
            android:name=".ServiceTest"
            android:exported="false" />
    </application>


</manifest>
